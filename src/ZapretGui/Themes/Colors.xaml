<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="SidebarColor">#202225</Color>
  <Color x:Key="ContentColor">#2f3136</Color>
  <Color x:Key="AccentColor">#5865f2</Color>
  <Color x:Key="TextPrimaryColor">#f4f5fb</Color>
  <Color x:Key="TextSecondaryColor">#9da5b1</Color>

  <SolidColorBrush x:Key="SidebarBackground" Color="{StaticResource SidebarColor}" />
  <SolidColorBrush x:Key="ContentBackground" Color="{StaticResource ContentColor}" />
  <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
  <SolidColorBrush x:Key="TextPrimary" Color="{StaticResource TextPrimaryColor}" />
  <SolidColorBrush x:Key="TextSecondary" Color="{StaticResource TextSecondaryColor}" />
</ResourceDictionary>
